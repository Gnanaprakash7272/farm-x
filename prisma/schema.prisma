generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Farm {
  id        Int      @id @default(autoincrement())
  name      String
  crop      String
  area      Float
  createdAt DateTime @default(now())
}

model User {
  id          Int      @id @default(autoincrement())
  phone       String?  @unique
  language    String?
  permissions Json?
  createdAt   DateTime @default(now())
}
